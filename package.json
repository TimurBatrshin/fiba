        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "axios": "^1.8.4",
        "bootstrap": "^5.3.5",
        "express": "^5.1.0",
        "jwt-decode": "^4.0.0",
        "minimatch": "^10.0.1",
        "portfinder": "^1.0.35",
        "react": "^18.3.1",
        "react-big-calendar": "^1.18.0",
        "react-bootstrap": "^2.10.9",
        "react-calendar": "^5.1.0",
        "react-dom": "^18.3.1",
        "react-router-dom": "^7.4.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
